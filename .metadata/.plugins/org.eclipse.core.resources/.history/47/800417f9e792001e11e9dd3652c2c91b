package Model;

public class Produto {
	private int qtd_min;
	private double Cod_Barras;
	private String Descricao;
	private String Un_Medida;
	private float Preco_Custo;
	private float Preco_Venda;
	private int Qtd_Estoque;
	private int Codigo;
	
	/**
	 * Construtor Default de Produto
	 */
	public Produto() {
		
	}
	/**
	 * @param qtd_min
	 * @param Cod_Barras
	 * @param Descricao
	 * @param Un_Medida
	 * @param Preco_Custo
	 * @param Preco_Venda
	 * @param Qtd_Estoque
	 * @param Codigo
	 * Construtor de Produto
	 */
	public Produto(int qtd_min, double Cod_Barras,String Descricao,String Un_Medida,float Preco_Custo, float Preco_Venda, int Qtd_Estoque, int Codigo) {
		super();
		setQtd_min(qtd_min);
		setCod_Barras(Cod_Barras);
		setDescricao(Descricao);
		setUn_Medida(Un_Medida);
		setPreco_Custo(Preco_Custo);
		setPreco_Venda(Preco_Venda);
		setQtd_Estoque(Qtd_Estoque);
		setCodigo(Codigo);
		
	}
	public int getQtd_min() {
		return qtd_min;
	}
	public void setQtd_min(int qtd_min) {
		this.qtd_min = qtd_min;
	}
	public double getCod_Barras() {
		return Cod_Barras;
	}
	public void setCod_Barras(double cod_Barras) {
		Cod_Barras = cod_Barras;
	}
	public String getDescricao() {
		return Descricao;
	}
	public void setDescricao(String descricao) {
		Descricao = descricao;
	}
	public String getUn_Medida() {
		return Un_Medida;
	}
	public void setUn_Medida(String un_Medida) {
		Un_Medida = un_Medida;
	}
	public float getPreco_Custo() {
		return Preco_Custo;
	}
	public void setPreco_Custo(float preco_Custo) {
		Preco_Custo = preco_Custo;
	}
	public float getPreco_Venda() {
		return Preco_Venda;
	}
	public void setPreco_Venda(float preco_Venda) {
		Preco_Venda = preco_Venda;
	}
	public int getQtd_Estoque() {
		return Qtd_Estoque;
	}
	public void setQtd_Estoque(int qtd_Estoque) {
		Qtd_Estoque = qtd_Estoque;
	}
	public int getCodigo() {
		return Codigo;
	}
	public void setCodigo(int codigo) {
		Codigo = codigo;
	}
	
	
}
